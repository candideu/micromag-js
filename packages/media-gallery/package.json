    "scripts": {
        "clean": "rm -rf scss && rm -rf assets && rm -rf lib && rm -rf es",
        "rollup": "NODE_ENV=production ../../node_modules/.bin/rollup --config ../../rollup.config.js",
        "scss": "mkdir scss && cp ./src/styles/styles.scss ./scss/styles.scss",
        "intl": "node ../../scripts/build-package-intl-translations.js",
        "build": "npm run rollup && npm run scss && npm run intl",
        "prepare": "npm run clean && npm run build",
        "start": "PUBLIC_URL=http://localhost/ react-scripts start"
    },
