        "@micromag/element-media-controls": "^0.1.24",
        "@micromag/element-text": "^0.1.24",
        "@micromag/element-video": "^0.1.24",
        "classnames": "^2.2.6",
        "lodash": "^4.17.15",
        "prop-types": "^15.7.2"
    },
