    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "lib",
        "es",
        "assets",
        "scss"
    ],
    "scripts": {
        "clean": "rm -rf scss && rm -rf assets && rm -rf lib && rm -rf es",
        "rollup": "NODE_ENV=production ../../node_modules/.bin/rollup --config ../../rollup.config.js",
        "intl": "node ../../scripts/build-package-intl-translations.js",
        "scss": "mkdir scss && cp ./src/styles/styles.scss scss",
        "css:mkdir": "mkdir -p ./assets/css/",
        "css:copy": "cp es/styles.css ./assets/css/styles.css",
        "css:clean": "rm -f es/styles.css && rm -f lib/styles.css",
        "css": "npm run css:mkdir && npm run css:copy && npm run css:clean",
        "build": "npm run rollup && npm run scss && npm run css && npm run intl",
        "prepare": "npm run clean && npm run build",
        "start": "PUBLIC_URL=http://localhost/ react-scripts start"
    },
    "devDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    },
    "peerDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.8.7",
        "@folklore/size": "^0.1.11",
        "@fortawesome/fontawesome-svg-core": "^1.2.28",
        "@fortawesome/free-solid-svg-icons": "^5.13.0",
        "@fortawesome/react-fontawesome": "^0.1.9",
        "@micromag/core": "^0.1.29",
        "@micromag/elements": "^0.1.29",
        "@micromag/fields": "^0.1.29",
        "@micromag/schemas": "^0.1.29",
        "@micromag/screens": "^0.1.29",
        "@micromag/viewer": "^0.1.29",
        "classnames": "^2.2.6",
        "lodash": "^4.17.15",
        "prop-types": "^15.7.2",
        "react-addons-css-transition-group": "^15.6.2",
        "react-intl": "^4.3.1",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-sortablejs": "^5.0.5",
        "sortablejs": "^1.10.0",
        "uuid": "^7.0.3"
    },
    "publishConfig": {
        "access": "public"
    },
