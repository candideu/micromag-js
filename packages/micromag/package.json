    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "lib",
        "es",
        "assets"
    ],
    "scripts": {
        "clean": "rm -rf lib && rm -rf es",
        "rollup": "NODE_ENV=production ../../node_modules/.bin/rollup --config ../../rollup.config.js",
        "sass:vendor": "../../scripts/build-sass.js ./src/styles/vendor.scss ./assets/css/vendor.css",
        "sass:styles": "../../scripts/build-sass.js ./src/styles/styles.scss ./assets/css/styles.css",
        "sass": "npm run sass:vendor && npm run sass:styles",
        "build": "npm run rollup && npm run sass",
        "intl": "node ../../scripts/build-package-intl-translations.js",
        "prepare": "npm run clean && npm run build && npm run intl"
    },
    "devDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    },
    "peerDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.8.7",
        "@micromag/app": "^0.1.9",
        "@micromag/core": "^0.1.9",
        "@micromag/editor": "^0.1.9",
        "bootstrap": "^4.4.1"
