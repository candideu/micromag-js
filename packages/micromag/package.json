    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "lib",
        "es",
        "assets",
        "scss"
    ],
    "scripts": {
        "clean": "rm -rf assets && rm -rf scss && rm -rf lib && rm -rf es",
        "rollup": "NODE_ENV=production ../../node_modules/.bin/rollup --config ../../rollup.config.js",
        "css:vendor": "../../scripts/build-sass.js ./src/styles/vendor.scss ./assets/css/vendor.css",
        "css:styles": "../../scripts/build-sass.js ./src/styles/styles.scss ./assets/css/styles.css",
        "css": "npm run css:vendor && npm run css:styles",
        "scss": "mkdir scss && cp ./src/styles/*.scss scss",
        "intl": "node ../../scripts/build-package-intl-translations.js",
        "build": "npm run rollup && npm run css && npm run scss && npm run intl",
        "prepare": "npm run clean && npm run build"
    },
    "devDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    },
    "peerDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.8.7",
        "@micromag/app": "^0.1.21",
        "@micromag/core": "^0.1.18",
        "@micromag/editor": "^0.1.21",
        "bootstrap": "^4.4.1"
    },
    "gitHead": "56ea5e35c4d053f49e1272568854e542bfe6e6cf"
}
